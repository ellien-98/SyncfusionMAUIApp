<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="SyncfusionMAUIApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:SyncfusionMAUIApp"
    Shell.FlyoutBehavior="Flyout">

<ShellContent Title="Main Page" ContentTemplate="{DataTemplate local:MainPage}" Route="MainPage" />
<ShellContent Title = "Autocomplete" ContentTemplate = "{DataTemplate local:AutocompleteFeatures}" Route ="Autocomplete" /> 
<ShellContent Title="PersonalInformation" ContentTemplate="{DataTemplate local:PersonalInformation}" Route="PersonalInformation" />
<ShellContent Title="PersonalInformationE" ContentTemplate="{DataTemplate local:PersonalInformationE}" Route="PersonalInformationE" />
<ShellContent Title = "DataGrid" ContentTemplate = "{DataTemplate local:DataGridFeatures}" Route ="DataGrid" /> 
<ShellContent Title = "ListView" ContentTemplate = "{DataTemplate local:ListViewFeatures}" Route ="ListView" /> 
<ShellContent Title = "ComboBox" ContentTemplate = "{DataTemplate local:ComboBoxFeatures}" Route ="ComboBox" /> 
<ShellContent Title = "DataForm" ContentTemplate = "{DataTemplate local:DataFormFeatures}" Route ="DataForm" /> 
<ShellContent Title = "Scheduler" ContentTemplate = "{DataTemplate local:SchedulerFeatures}" Route ="Scheduler" /> 
<ShellContent Title = "Button" ContentTemplate = "{DataTemplate local:ButtonFeatures}" Route ="Button" /> 
<ShellContent Title = "PDF Viewer" ContentTemplate = "{DataTemplate local:PDFViewerFeatures}" Route ="PDF Viewer" /> 
<ShellContent Title = "Circular Charts" ContentTemplate = "{DataTemplate local:CircularChartsFeatures}" Route ="Circular Charts" /> 
<ShellContent Title = "Text Input Layout" ContentTemplate = "{DataTemplate local:TextInputLayoutFeatures}" Route ="Text Input Layout" /> 
<ShellContent Title = "Pyramid Charts" ContentTemplate = "{DataTemplate local:PyramidChartsFeatures}" Route ="Pyramid Charts" /> 
<ShellContent Title = "Radial Gauge" ContentTemplate = "{DataTemplate local:RadialGaugeFeatures}" Route ="Radial Gauge" /> 
<ShellContent Title = "Linear Gauge" ContentTemplate = "{DataTemplate local:LinearGaugeFeatures}" Route ="Linear Gauge" /> 
<ShellContent Title = "Maps" ContentTemplate = "{DataTemplate local:MapsFeatures}" Route ="Maps" /> 
<ShellContent Title = "TreeMap" ContentTemplate = "{DataTemplate local:TreeMapFeatures}" Route ="TreeMap" /> 


</Shell>
